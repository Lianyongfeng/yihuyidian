              <view class='order'>
                <view catchtap='sign' hidden='{{bItem.number==0}}' data-item-index="{{index}}" data-parentIndex="{{parentIndex}}">
                  <i class="iconfont icon-jianhao"></i>
                </view>
                <view hidden='{{bItem.number==0}}' data-index='{{index}}'>{{bItem.number}}</view>
                <view catchtap='add' data-item-index="{{index}}" data-parentIndex="{{parentIndex}}">
