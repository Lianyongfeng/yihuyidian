          </view>
          <view class='total' bindtap='cartMore'>
            <view hidden="{{totalPrice==0}}">￥{{totalPrice}}</view>
            <view hidden="{{totalPrice==0}}">{{peisong}}</view>
            <view hidden="{{totalPrice!=0}}">{{no}}</view>
            </view>
