  calTotalPrice() {
    // 计算总价
    var carArray = this.carArray;
    var totalPrice = 0;
    var totalCount = 0;
    for (var i = 0; i < carArray.length; i++) {
      totalPrice += carArray[i].price * carArray[i].number;
      totalCount += carArray[i].number;
    }
    this.totalPrice = totalPrice;
    this.totalCount = totalCount;
  }
  cartShow(fold) {
    if (fold == false) {
      this.cartMoreClass = 'cart-more-active';
    } else {
      this.cartMoreClass = 'cart-more';
    }
  }
  cartAdd(e) {
    // 增加
    var index = e.currentTarget.dataset.itemIndex;
    var parentIndex = e.currentTarget.dataset.parentindex;
    this.data.foods[parentIndex].details[index].number++;
    var number = this.data.foods[parentIndex].details[index].number;
    var price = this.data.foods[parentIndex].details[index].price;
    var name = this.data.foods[parentIndex].details[index].name;
    var pic = this.data.foods[parentIndex].details[index].pic;
    var mon = this.data.foods[parentIndex].details[index].mon;
    var flavor = this.data.foods[parentIndex].details[index].flavor;
    var evaluate = this.data.foods[parentIndex].details[index].evaluate;
    var mark = 'a' + index + 'b' + parentIndex;
    var obj = {
      price: price,
      number: number,
      mark: mark,
      name: name,
      parentIndex: parentIndex,
      index: index,
      pic:pic,
      flavor:flavor,
      mon:mon,
      evaluate:evaluate
    };
    var carArray1 = this.carArray.filter(item => item.mark != mark);
    carArray1.push(obj);
    this.carArray = carArray1;
  }
  cartSign(e) {
    // 减少
    var index = e.currentTarget.dataset.itemIndex;
    var parentIndex = e.currentTarget.dataset.parentindex;
    this.data.foods[parentIndex].details[index].number--;
    var number = this.data.foods[parentIndex].details[index].number;
    var price = this.data.foods[parentIndex].details[index].price;
    var name = this.data.foods[parentIndex].details[index].name;
    var pic = this.data.foods[parentIndex].details[index].pic;
    var flavor = this.data.foods[parentIndex].details[index].flavor;
    var mon = this.data.foods[parentIndex].details[index].mon;
    var evaluate = this.data.foods[parentIndex].details[index].evaluate;
    var mark = 'a' + index + 'b' + parentIndex;
    var obj = {
      price: price,
      number: number,
      mark: mark,
      name: name,
      parentIndex: parentIndex,
      index: index,
      pic:pic,
      flavor:flavor,
      mon:mon,
      evaluate:evaluate
    };
    var carArray1 = this.carArray.filter(item => item.mark != mark);
    carArray1.push(obj);
    this.carArray = carArray1;
    this.foods = this.foods;
    // 关闭弹窗
    var count1 = 0;
    for (let i = 0; i < carArray1.length; i++) {
      if (carArray1[i].number == 0) {
        count1++;
      }
    }
    if (count1 == carArray1.length) {
      console.log(number);
      if (number == 0) {
        console.log(number);
        this.data.carArray = [];
        this.cartMoreClass = 'cart-more';
        this.totalPrice = 0;
        this.totalCount = 0;
      }
    }
