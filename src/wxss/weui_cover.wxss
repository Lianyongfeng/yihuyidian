.weui-cells__title + .weui-cells {
    margin-top: 0 !important;
}
.weui-cells {
  margin-top: 20rpx !important;
  font-size: 30rpx !important;
}

.weui-cell:before {
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
}
.weui-btn {
  position: relative;
  display: block;
  margin-left: auto;
  margin-right: auto;
  padding-left: 14px;
  padding-right: 14px;
  box-sizing: border-box;
  text-align: center;
  text-decoration: none;
  line-height: 2.55555556;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  overflow: hidden;
}

.weui-btn_primary {
  background-color: #ff4258;
  color: #fff;
}
.weui-loadmore {
  width: 100%;
  line-height: 1.3em;
  margin: 1.5em auto;
  text-align: center;
  color: #c7c7c7;
}

.weui-panel:before {

  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
}

.weui-panel:after {
 
  border-bottom: 1px solid #e5e5e5;
  color: #e5e5e5;
}

.weui-cell_vcode {
  padding-top: 0;
  padding-right: 0;
  padding-bottom: 0;
}
